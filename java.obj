class Employe{
    int id;
    String name;
    int rollno;
   public  Employe(int salary,String name){
       System.out.println(salary);
        System.out.println(name);
       
   }
    void display(int id,String name,int rollno){
        this.id=id;
        this.name=name;
        this.rollno=rollno;

    }
}
class Main{
