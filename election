import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		  int a[]=new int[3];
		  int max=-999;
		  for(int i=0;i<3;i++){
		      a[i]=sc.nextInt();
		    if(a[i]>max){
		        max=a[i];
		    }  
		}
		for(int i=0;i<3;i++){
		    if(max==a[i]){
		        System.out.print(0+" ");
		    }else{
		        System.out.print(max+1-a[i]+" ");
		    }
		}
		System.out.println();
	}
}
}
