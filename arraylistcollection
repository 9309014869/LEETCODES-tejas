import java.util.*;
public class HelloWorld{
    
     public static void main(String []args){
        ArrayList<Integer>a1 = new ArrayList<>(20);
         ArrayList<Integer>al1 = new ArrayList<>();
         a1.add(12);
          a1.add(112);
          al1.addAll(1,2,3);
          a1.addAll(al1);
         System.out.println(a1);
     }
}
